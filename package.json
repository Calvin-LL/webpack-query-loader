    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "memfs": "^3.2.0",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.0",
    "ts-jest": "^26.1.0",
